target 'Runner' do
  # Brug dynamiske frameworks
  use_frameworks!

  # Firebase Pods for Runner
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore' # Til Firestore, hvis det er n√∏dvendigt
  pod 'Firebase/Analytics'

  target 'RunnerTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
